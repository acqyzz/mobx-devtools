{
  "name": "extension",
  "version": "1.0.0",
  "description": "mobx devtool extension",
  "scripts": {
    "build": "concurrently pnpm:build-*",
    "dev-chrome": "RUN_ENV=CHROME webpack -c ./scripts/webpack.config.dev.js",
    "dev-firefox": "RUN_ENV=FIREFOX webpack -c ./scripts/webpack.config.dev.js",
    "dev-edge": "RUN_ENV=EDGE webpack -c ./scripts/webpack.config.dev.js",
    "dev-electron": "RUN_ENV=ELECTRON webpack -c ./scripts/webpack.config.dev.js",
    "build-chrome": "RUN_ENV=CHROME webpack -c ./scripts/webpack.config.prod.js",
    "build-firefox": "RUN_ENV=FIREFOX webpack -c ./scripts/webpack.config.prod.js",
    "build-edge": "RUN_ENV=EDGE webpack -c ./scripts/webpack.config.prod.js",
    "build-electron": "RUN_ENV=ELECTRON webpack -c ./scripts/webpack.config.prod.js",
    "dev": "concurrently pnpm:dev-*"
  },
  "keywords": [
    "mobx",
    "devtool",
    "devtools"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-proposal-decorators": "^7.23.6",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/chrome": "^0.0.254",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@types/react-window": "^1.8.8",
    "antd": "^5.12.5",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "chrome-types": "^0.1.246",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.5.4",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.3",
    "thread-loader": "^4.0.2",
    "typescript": "^5.3.3",
    "patch-obj": "workspace:*",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@msgpack/msgpack": "2",
    "@types/uuid": "^9.0.8",
    "flatted": "^3.2.9",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "rc-tabs": "^12.15.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.10",
    "uuid": "^9.0.1"
  }
}
